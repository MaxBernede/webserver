#!/usr/bin/python3
print ("<!DOCTYPE html>\n")
print ("<html>")
print ("Content-Type : text/html")

# then comes the rest hyper-text documents
print ("<html>")
print ("<head>")
print ("<title>CGI-Python-Program</title>")
print ("<head>")
print ("<body>")
print ("<h3>Hello, from our webserver!</h3>")
print ("</body>")
print ("</html>")